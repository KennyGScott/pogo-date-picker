{
  "extends": "../tslint.json",
  "rules": {
    "no-trailing-whitespace": false,
    "one-line": false,
    "no-namespace": false,
    "variable-name": false,
    "prefer-for-of": false,
    "object-literal-shorthand": false,
    "use-host-property-decorator": false,
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": false
  }
}
